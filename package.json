{
  "name": "astro_dev",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"src/**/*.{js,ts,astro}\"",
    "lint:astro": "astro check",
    "lint:prettier": "prettier --check src",
    "lint:markuplint": "markuplint \"src/**/*.astro\"",
    "lint:stylelint": "stylelint \"src/**/*.astro\"",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "prettier --write src",
    "fix:stylelint": "npm run lint:stylelint -- --fix"
  },
  "dependencies": {
    "@astrojs/image": "^0.17.1",
    "@astrojs/preact": "^2.2.1",
    "@astrojs/sitemap": "^1.0.0",
    "astro": "^2.7.2",
    "astro-compress": "^1.1.24",
    "astro-purgecss": "^2.0.0",
    "astro-relative-links": "^0.3.4",
    "astro-seo": "^0.7.0",
    "gsap": "^3.12.2",
    "preact": "^10.15.1",
    "preact-compat": "^3.19.0",
    "react-transition-group": "^4.4.5",
    "sass": "^1.63.6",
    "sharp": "^0.32.4"
  },
  "devDependencies": {
    "@markuplint/astro-parser": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-plugin-astro": "^0.27.0",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "markuplint": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.1",
    "prettier-plugin-astro": "^0.10.0",
    "stylelint": "^15.0.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^33.0.0"
  },
  "overrides": {
    "react": "npm:@preact/compat@latest",
    "react-dom": "npm:@preact/compat@latest"
  }
